{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Issuer": "IsaArtTattoo",
    "Audience": "IsaArtTattoo",
    "Key": "50052fd97ba5f70a000af4f72ebde6efd72be43d9ad8dd552d45c548cd0ebd91"
  },
  "ReverseProxy": {
    "Routes": {
      "identity-auth": {
        "ClusterId": "identity-cluster",
        "RateLimiterPolicy": "anonymous",
        "Match": {
          "Path": "/api/v{version}/Auth/{**catch-all}"
        }
      },
      "identity-users": {
        "ClusterId": "identity-cluster",
        "AuthorizationPolicy": "admin",
        "RateLimiterPolicy": "authenticated",
        "Match": {
          "Path": "/api/v{version}/Users/{**catch-all}"
        }
      },
      "identity-roles": {
        "ClusterId": "identity-cluster",
        "AuthorizationPolicy": "admin",
        "RateLimiterPolicy": "authenticated",
        "Match": {
          "Path": "/api/v{version}/Roles/{**catch-all}"
        }
      },
      "orders-user": {
        "ClusterId": "orders-cluster",
        "AuthorizationPolicy": "authenticated",
        "RateLimiterPolicy": "authenticated",
        "Match": {
          "Path": "/api/v{version}/orders/{**catch-all}"
        },
        "Transforms": [
          {
            "RequestHeadersCopy": "true"
          }
        ]
      },
      "orders-admin": {
        "ClusterId": "orders-cluster",
        "AuthorizationPolicy": "admin",
        "RateLimiterPolicy": "authenticated",
        "Match": {
          "Path": "/api/v{version}/admin/orders/{**catch-all}"
        },
        "Transforms": [
          {
            "RequestHeadersCopy": "true"
          }
        ]
      },
      "catalog-public": {
        "ClusterId": "catalog-cluster",
        "RateLimiterPolicy": "anonymous",
        "Match": {
          "Path": "/api/catalog/{**catch-all}"
        }
      },
      "catalog-public-v1": {
        "ClusterId": "catalog-cluster",
        "RateLimiterPolicy": "anonymous",
        "Match": {
          "Path": "/api/v{version}/catalog/{**catch-all}"
        }
      },
      "catalog-admin": {
        "ClusterId": "catalog-cluster",
        "AuthorizationPolicy": "admin",
        "RateLimiterPolicy": "authenticated",
        "Match": {
          "Path": "/api/admin/catalog/{**catch-all}"
        },
        "Transforms": [
          {
            "RequestHeadersCopy": "true"
          }
        ]
      },
      "catalog-admin-v1": {
        "ClusterId": "catalog-cluster",
        "AuthorizationPolicy": "admin",
        "RateLimiterPolicy": "authenticated",
        "Match": {
          "Path": "/api/v{version}/admin/catalog/{**catch-all}"
        },
        "Transforms": [
          {
            "RequestHeadersCopy": "true"
          }
        ]
      }
    },
    "Clusters": {
      "identity-cluster": {
        "Destinations": {
          "identity-service": {
            "Address": "http://identity-api"
          }
        }
      },
      "orders-cluster": {
        "Destinations": {
          "orders-service": {
            "Address": "http://orders-api"
          }
        }
      },
      "catalog-cluster": {
        "Destinations": {
          "catalog-service": {
            "Address": "http://catalog-api"
          }
        }
      }
    }
  }
}
